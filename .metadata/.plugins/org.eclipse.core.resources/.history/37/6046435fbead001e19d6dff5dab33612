package com.myweb.www.domain;

public class PagingVO {
	
	private int pageNo; // 현재 페이지 번호
	private int qty; // 한페이지당 표시될 리스트 개수
	
	public PagingVO() {
		this.pageNo = 1;
		this.qty = 10;
		// => 1 pageNo 에 10개 qty
	}
	
	public Paging(int pageNo, int qty) {
		this.pageNo = pageNo;
		this.qty = qty;
	}
	
}
